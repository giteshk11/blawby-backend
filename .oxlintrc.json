{
  "plugins": ["import", "typescript", "node", "unicorn"],
  "env": {
    "node": true,
    "es2022": true
  },
  "rules": {
    "correctness": "error",
    "suspicious": "error",
    "pedantic": "error",
    "style": "error",
    "perf": "error",
    "nursery": "off"
  },
  "allow": [
    "no-console",
    "no-debugger",
    "no-alert",
    "no-plusplus",
    "no-unsafe-member-access",
    "no-unsafe-return",
    "no-misused-spread",
    "no-non-null-assertion",
    "no-unnecessary-condition",
    "prefer-nullish-coalescing",
    "restrict-template-expressions",
    "ban-ts-comment",
    "no-empty-function",
    "no-empty",
    "no-var",
    "no-bitwise",
    "no-eq-null",
    "no-iterator",
    "no-proto",
    "no-regex-spaces",
    "no-undefined",
    "no-unused-expressions",
    "no-void",
    "no-async-await",
    "no-optional-chaining",
    "no-rest-spread-properties",
    "explicit-function-return-type",
    "no-dynamic-delete",
    "no-empty-object-type",
    "no-import-type-side-effects",
    "no-namespace",
    "no-non-null-asserted-nullish-coalescing",
    "no-require-imports",
    "no-var-requires",
    "prefer-literal-enum-member",
    "no-abusive-eslint-disable",
    "no-anonymous-default-export",
    "no-array-for-each",
    "no-array-reduce",
    "no-document-cookie",
    "no-magic-array-flat-depth",
    "no-nested-ternary",
    "no-process-exit",
    "prefer-modern-math-apis",
    "prefer-node-protocol",
    "prefer-number-properties"
  ],
  "deny": [
    "no-unused-vars",
    "no-explicit-any",
    "no-await-in-loop",
    "no-useless-call",
    "no-accumulating-spread",
    "no-array-index-key",
    "jsx-no-jsx-as-prop",
    "jsx-no-new-array-as-prop",
    "jsx-no-new-function-as-prop",
    "jsx-no-new-object-as-prop",
    "prefer-array-find",
    "prefer-set-has",
    "no-var",
    "no-eval",
    "no-implied-eval",
    "no-new-func",
    "no-script-url",
    "no-self-compare",
    "no-throw-literal",
    "no-unmodified-loop-condition",
    "no-unused-labels",
    "no-useless-concat",
    "no-useless-escape",
    "no-useless-return",
    "prefer-promise-reject-errors",
    "prefer-regex-literals",
    "prefer-template",
    "sort-imports",
    "sort-keys",
    "yoda",
    "consistent-type-specifier-style",
    "exports-last",
    "first",
    "group-exports",
    "no-anonymous-default-export",
    "no-mutable-exports",
    "no-named-default",
    "no-namespace",
    "consistent-test-it",
    "max-expects",
    "max-nested-describe",
    "no-alias-methods",
    "no-confusing-set-timeout",
    "no-deprecated-functions",
    "no-done-callback",
    "no-duplicate-hooks",
    "no-identical-title",
    "no-interpolation-in-snapshots",
    "no-jasmine-globals",
    "no-large-snapshots",
    "no-mocks-import",
    "no-restricted-jest-methods",
    "no-restricted-matchers",
    "no-test-prefixes",
    "no-test-return-statement",
    "no-untyped-mock-factory",
    "prefer-called-with",
    "prefer-comparison-matcher",
    "prefer-each",
    "prefer-equality-matcher",
    "prefer-expect-resolves",
    "prefer-hooks-in-order",
    "prefer-hooks-on-top",
    "prefer-jest-mocked",
    "prefer-lowercase-title",
    "prefer-mock-promise-shorthand",
    "prefer-spy-on",
    "prefer-strict-equal",
    "prefer-to-be",
    "prefer-to-contain",
    "prefer-to-have-length",
    "prefer-todo",
    "require-hook",
    "require-top-level-describe",
    "no-exports-assign",
    "avoid-new",
    "no-nesting",
    "no-return-wrap",
    "param-names",
    "prefer-await-to-callbacks",
    "prefer-await-to-then",
    "prefer-catch",
    "jsx-boolean-value",
    "jsx-curly-brace-presence",
    "no-set-state",
    "prefer-es6-class",
    "self-closing-comp",
    "adjacent-overload-signatures",
    "array-type",
    "ban-tslint-comment",
    "consistent-generic-constructors",
    "consistent-indexed-object-style",
    "consistent-type-definitions",
    "consistent-type-imports",
    "no-empty-interface",
    "no-inferrable-types",
    "prefer-for-of",
    "prefer-function-type",
    "prefer-namespace-keyword",
    "catch-error-name",
    "consistent-date-clone",
    "consistent-existence-index-check",
    "empty-brace-spaces",
    "error-message",
    "filename-case",
    "no-array-method-this-argument",
    "no-await-expression-member",
    "no-console-spaces",
    "no-null",
    "no-unreadable-array-destructuring",
    "no-zero-fractions",
    "number-literal-case",
    "numeric-separators-style",
    "prefer-array-flat-map",
    "prefer-array-index-of",
    "prefer-dom-node-text-content",
    "prefer-global-this",
    "prefer-includes",
    "prefer-logical-operator-over-ternary",
    "prefer-modern-dom-apis",
    "prefer-negative-index",
    "prefer-object-from-entries",
    "prefer-optional-catch-binding",
    "prefer-reflect-apply",
    "prefer-spread",
    "prefer-string-raw",
    "prefer-string-trim-start-end",
    "prefer-structured-clone",
    "require-array-join-separator",
    "switch-case-braces",
    "text-encoding-identifier-case",
    "throw-new-error",
    "no-import-node-test",
    "prefer-to-be-falsy",
    "prefer-to-be-object",
    "prefer-to-be-truthy",
    "no-unsafe-argument",
    "no-unsafe-assignment",
    "no-unsafe-call"
  ]
}
